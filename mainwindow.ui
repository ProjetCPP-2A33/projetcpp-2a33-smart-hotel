QTabBar::tab:pressed {
    background-color: #353FF2; /* Conserver la même couleur que pour l'onglet actif */
}


/* Style du contenu des onglets */
QTabWidget::pane {
    border: 2px solid #000000; /* Bordure bleu foncé pour le contenu des onglets */
    border-top: 0; /* Supprimer la bordure supérieure */
}

